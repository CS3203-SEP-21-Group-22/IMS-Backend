<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <modules>
      <!-- Remove WebDAVModule to allow PUT, DELETE methods -->
      <remove name="WebDAVModule" />
    </modules>
    <handlers>
      <!-- Remove WebDAV handler -->
      <remove name="WebDAV" />
    </handlers>
  </system.webServer>
</configuration>
