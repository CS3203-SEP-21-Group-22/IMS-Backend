{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DBConnection": "AZURE_SQL_SERVER_CONNECTION_STRING",
    "ApplicationInsights": "APPLICATION_INSIGHTS_CONNECTION_STRING"
  },
  "AuthenticationServer": {
    "Endpoint": "AUTH_SERVER_BASE_URL",
    "ClientId": "AUTH_CLIENT_ID",
    "ClientSecret": "AUTH_CLIENT_SECRET"
  },
  "Jwt": {
    "Key": "AUTH_CLIENT_SECRET",
    "Issuer": "JwtIssuer",
    "Audience": "JwtAudience",
    "Subject": "JwtSubject"
  },
  "AzureBlobStorage": {
    "ContainerName": "AZURE_CONTAINER_NAME",
    "ConnectionString": "AZURE_CONTAINER_CONNECTION_STRING"
  },
  "EmailClient": {
    "APIKey": "SENDGRID_API_KEY",
    "SenderEmail": "SENDGRID_SENDER_EMAIL"
  },
  "QRToken": {
    "Secret": "QR_TOKEN_SECRET"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
